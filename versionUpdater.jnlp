<resources>
    <j2se href="http://java.sun.com/products/autodl/j2se" onclick="javascript:mytracker(this.href);" version="1.5+"/>
    <jar href="LogbinUpdater.jar"/>
</resources>



<application-desc main-class="updater.VersionUpdater"/>

</jnlp>
